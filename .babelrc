{
  "presets": [
    [
      "@babel/preset-react",
      {
        "loose": true,
        "modules": "commonjs",
        "targets": {
          "electron": "7.0",
          "node": "current"
        }
      }
    ],
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", {
      "decoratorsBeforeExport": true
    }],
    "@babel/plugin-syntax-export-default-from",
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "root": ["src"]
    }]
  ]
}
